{
  "Routes": [
    {
      "DownstreamPathTemplate": "/api/v1/Pay",
      "DownstreamScheme": "https",
      "ServiceName": "ApiAggregator",
      "UpstreamPathTemplate": "/CalculatePayCheck",
      "UpstreamHttpMethod": [ "GET", "POST", "PUT", "DELETE" ],
      "FileCacheOptions": { "TtlSeconds": 30 },
      "AuthenticationOptions": {
        "AuthenticationProviderKey": "IdentityApiKey",
        "AllowedScopes": []
      }
    },
    {
      "DownstreamPathTemplate": "/api/v1/Catalog/{catchAll}",
      "DownstreamScheme": "https",
      "ServiceName": "ApiAggregator",
      "UpstreamPathTemplate": "/CalculatePayCheck/{catchAll}",
      "UpstreamHttpMethod": [ "GET", "POST", "PUT", "DELETE" ],
      "FileCacheOptions": { "TtlSeconds": 30 },
      "AuthenticationOptions": {
        "AuthenticationProviderKey": "IdentityApiKey",
        "AllowedScopes": []
      }
    }
  ],
  "GlobalConfiguration": {
    "ServiceDiscoveryProvider": {
      "Host": "consul",
      "Port": 8500,
      "Type": "Consul"
    }
  }
}
